---
import Layout from "../../../layouts/Layout.astro";
import { Image } from "astro:assets";

import portrait from "../../../images/profile-pic/portrait.png";
import logistic_bg from "../../../images/bg/logistic_solution_mobile.webp";
import logistic_bg_lg from "../../../images/bg/logistic_solution_lg.webp";
import ButtonOutline from "../../../components/ButtonOutline.astro";
import BenefitsLogistics from "../../../components/BenefitsLogistics.astro";
import SourceImageTwo from "../../../components/SourceImageTwo.astro";
---

<style>
  .card-solution {
    @apply flex flex-col items-start justify-start gap-y-5;
  }
  .card-solution svg {
    @apply size-10 fill-white stroke-white;
  }

  .card-solution > div {
    @apply space-y-2;
  }

  .card-solution h4 {
    @apply text-2xl ltr:text-left rtl:text-right;
  }
  .card-solution p {
    @apply ltr:text-left rtl:text-right;
  }
</style>
<Layout title="Solutions | 3PL" lang="en">
  <section class="hero z-0" id="PL-hero">
    <div class="blurred-img bg-cover bg-center bg-blurredlogistic_solution">
      <SourceImageTwo
        mobileImgUrl={logistic_bg}
        desktopImgUrl={logistic_bg_lg}
        alt="logistic_solution_bg"
      />
    </div>
    <div
      id="hero-overlay"
      class="absolute top-0 left-0 size-full bg-black/70 xs:bg-transparent xs:bg-gradient-to-l from-black/50 from-0% to-black/85 to-80% z-0"
    >
    </div>
    <div
      class="fade container section-container flex flex-col z-0 justify-center h-full gap-5"
      data-animation="animate-fade animate-duration-[1500ms]"
    >
      <h1 class="text-3xl md:text-5xl text-white mt-10">Logistic Companies</h1>
      <h6
        class="text-base font-medium md:text-lg opacity-80 text-white lg:w-1/2"
      >
        Leajlakâ€™s Delivery Management System provides a comprehensive solution
        to address logistical challenges faced by companies, enhancing
        operational efficiency and streamlining delivery processes.
      </h6>
    </div>
  </section>
  <section class="bg-lightGray">
    <div class="container section-container space-y-24">
      <div
        class="fade flex flex-col items-center text-center gap-2"
        data-animation="animate-fade animate-duration-[1000ms]"
      >
        <h2>Efficient Logistics for 3PL Companies</h2>
        <h6 class="text-lg lg:w-2/3">
          Our platform optimizes routes and enhances driver coordination for
          faster, more cost-effective deliveries.
        </h6>
      </div>
      <div
        class="grid grid-cols-1 md:grid-cols-3 items-start justify-center gap-y-20 md:gap-x-12 md:gap-y-20"
