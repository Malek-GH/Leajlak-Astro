---
import { Image } from "astro:assets";

import Layout from "../../../layouts/Layout.astro";
import cost from "../../../images/bg/cost_saving.webp";
import experience from "../../../images/bg/customer experience.webp";
import scalability from "../../../images/bg/charts.webp";
import integration from "../../../images/bg/integration.webp";
import portrait from "../../../images/profile-pic/portrait.png";
import ButtonOutline from "../../../components/ButtonOutline.astro";
---

<Layout title="Solutions | Merchants" lang="en">
  <section
    class="bg-merchant_mobile sm:bg-merchant bg-cover bg-center lg:bg-left bg-no-repeat bg-fixed hero flex items-center"
    id="merchants-hero"
  >
    <div
      class="fade container section-container"
      data-animation="animate-fade animate-duration-[2000ms]"
    >
      <div class="flex flex-col gap-7 text-white text-left lg:w-[55%]">
        <h1 class="text-3xl md:text-5xl md:leading-tight">Merchants</h1>
        <h4 class="text-base md:text-xl opacity-80">
          Leajlak's solution revolutionizes order management, addressing the
          critical challenges faced by merchants and unlocking the potential for
          a seamless, efficient delivery ecosystem.
        </h4>
      </div>
    </div>
  </section>
  <section class="">
    <div
      class="container section-container flex flex-col items-center justify-center gap-20"
    >
      <div
        class="flex flex-col items-center text-center gap-2"
        data-animation="animate-fade animate-duration-[1500ms]"
      >
        <h2 class="text-primaryOrange">Our Solutions</h2>
        <p>Empowering Merchants with Seamless Delivery Solutions.</p>
      </div>
      <div
        class="grid grid-cols-1 md:grid-cols-2 items-center justify-center gap-y-20 md:gap-x-20 md:gap-y-5"
